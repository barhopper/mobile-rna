{
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "strict": 0
    },
    "plugins": ["jest", "plugin:react/recommended"],
    "extends": [
      "eslint:recommended",
      "eslint-config-prettier",
      "react",
      "react-native"
    ],
    "env": {
      "node": true,
      "browser": true,
      "react-native/react-native": true,
      "jest": true
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  }